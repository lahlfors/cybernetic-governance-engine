      "type": "heatmap",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 7
      },
      "targets": [
        {
          "expr": "rate(latency_currency_tax_bucket[5m])",
          "format": "heatmap",
          "refId": "A"
        }
      ]
    }
